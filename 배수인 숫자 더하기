import java.util.Scanner;

public class JAVA {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.print("합계의 시작값 ==> ");
		int j = sc.nextInt();
		System.out.print("합계의 끝값 ==> ");
		int b = sc.nextInt();
		System.out.print("배수 ==> ");
		int c = sc.nextInt();
		int h = 0;
		int a = j;
		
		while(a<=b) {
			if(a%c==0) {
				h+=a;
			}
			a++;
		}
		
		System.out.printf("%d부터 %d까지의 %d배수의 합계 ==> %d",j,b,c,h);
	}

}
